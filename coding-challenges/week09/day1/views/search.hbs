 <div class="jumbotron">
            
                    <h2>Book Library</h2>
                
        </div>
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                    <th scope="col">Title</th>  
                                    <th scope="col">Author</th>  
                                    <th scope="col">Country</th>  
                                    <th scope="col">language</th>  
                                    <th scope="col">Link</th>  
                                    <th scope="col">Page</th> 
                                    <th scope="col">Year</th> 
                            </tr>
                        </thead>
                    
                    <tbody>
                        {{#each data}}
                        <tr>
                             <th scope="row">{{this.title}}</th>
                             <td>{{this.author}}</td>
                             <td>{{this.country}}</td>
                             <td>{{this.language}}</td>
                             <td><a href = {{this.link}}>Link</a></td>
                             <td>{{this.pages}}</td>
                             <td>{{this.year}}</td>
                        </tr>
                        {{/each}}
                    </tbody> 
                    </table>
                </div>
            </div>
        </div>