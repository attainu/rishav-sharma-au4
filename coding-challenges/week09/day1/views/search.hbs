 <div class="jumbotron">
            <div class="row">
                <div class="col-md-6">
                    <h2>Book Library</h2>
                    <h6> Enter the Language</h6>
                    <form class="from-group">
                        <input type="text" class="form-control" id="search-keyword" placeholder="E.g.,English" required>
                    </form>
                    <button type="button" id="submit-btn" class="btn btn-info">Submit</button>
                </div>
            </div>
        </div>
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                    <th scope="col">Title</th>  
                                    <th scope="col">Author</th>  
                                    <th scope="col">Country</th>  
                                    <th scope="col">language</th>  
                                    <th scope="col">Link</th>  
                                    <th scope="col">Page</th> 
                                    <th scope="col">Year</th> 
                            </tr>
                        </thead>
                    </table>
                    <tbody>
                        {{#each data}}
                        <tr>
                             <td>{{this.title}}</td>
                             <td>{{this.author}}</td>
                             <td>{{this.country}}</td>
                             <td>{{this.language}}</td>
                             <td><a href = {{this.link}}>Link</a></td>
                             <td>{{this.pages}}</td>
                             <td>{{this.year}}</td>
                        </tr>
                        {{/each}}
                    </tbody>
                </div>
            </div>
        </div>      